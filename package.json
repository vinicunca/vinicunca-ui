{
  "name": "@praburangki/vinicunca",
  "version": "1.0.0",
  "description": "UI Components for Vue 3",
  "license": "MIT",
  "author": "praburangki<https://github.com/praburangki>",
  "main": "./dist/vinicunca.umd.js",
  "module": "./dist/vinicunca.es.js",
  "exports": {
    ".": {
      "import": "./dist/vinicunca.es.js",
      "require": "./dist/vinicunca.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:css": "esno src/designs/index.ts",
    "uno": "unocss \"src/components/**/*\" --out-file src/designs/styles/uno.css",
    "lint:eslint": "eslint src/**/*.{ts,vue}",
    "lint:prettier": "prettier -w -u src/**/*.{ts,vue} docs/src/**/*.{ts,vue}",
    "lint:stylelint": "stylelint **/*.{less,vue} --syntax less",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "cy:open": "cypress open-ct",
    "cy:run": "percy exec -- cypress run-ct"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.2",
    "@cypress/vue": "^3.0.5",
    "@iconify-json/carbon": "^1.0.12",
    "@percy/cli": "^1.0.0-beta.71",
    "@percy/cypress": "^3.1.1",
    "@praburangki/eslint-config": "^1.0.1",
    "@praburangki/web-utilities": "^1.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.9",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vueuse/head": "^0.7.4",
    "cssnano": "^5.0.14",
    "cypress": "^9.2.0",
    "eslint": "^8.2.0",
    "eslint-define-config": "^1.1.4",
    "eslint-plugin-cypress": "^2.12.1",
    "esno": "^0.13.0",
    "fs-extra": "^10.0.0",
    "happy-dom": "^2.25.1",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "prettier": "^2.4.1",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "typescript": "4.4.4",
    "unocss": "^0.20.0",
    "vite": "^2.7.3",
    "vite-plugin-dts": "^0.9.6",
    "vite-ssr": "^0.14.3",
    "vitest": "^0.0.109",
    "vue-router": "^4.0.12"
  },
  "dependencies": {
    "vue": "^3.2.26"
  }
}
