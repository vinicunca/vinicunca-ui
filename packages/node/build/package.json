  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vinicunca-cli build",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "@vinicunca/node-constants": "workspace:*",
    "@vinicunca/node-utils": "workspace:*",
    "@vinicunca/workspace": "workspace:*",
    "esbuild": "^0.14.48",
    "local-pkg": "^0.4.1",
    "tsup": "^6.1.3",
    "unconfig": "^0.3.5",
    "unplugin-vue": "^2.2.1"
  },
  "devDependencies": {
    "tsx": "^3.7.1",
    "vue": "^3.2.37"
  }
