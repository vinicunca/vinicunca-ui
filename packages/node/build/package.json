{
  "name": "@vinicunca/build",
  "type": "module",
  "version": "0.0.1",
  "description": "Build Toolchain for Vinicunca.",
  "license": "MIT",
  "homepage": "https://vinicunca.dev/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinicunca/vinicunca-ui.git",
    "directory": "packages/node/build"
  },
  "bugs": {
    "url": "https://github.com/vinicunca/vinicunca-ui/issues"
  },
  "keywords": [
    "vinicunca",
    "component library",
    "ui framework",
    "ui",
    "vue",
    "build",
    "core"
  ],
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./config": {
      "dev": "./src/config.ts",
      "import": "./dist/config.js"
    },
    "./*": "./*"
