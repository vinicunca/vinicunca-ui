{
  "name": "vinicunca",
  "version": "0.1.2",
  "description": "A Component Library for Vue 3",
  "keywords": [
    "vinicunca",
    "component library",
    "ui framework",
    "ui",
    "vue"
  ],
  "homepage": "https://vinicunca.dev/",
  "bugs": {
    "url": "https://github.com/vinicunca/vinicunca-ui/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinicunca/vinicunca-ui.git"
  },
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "unpkg": "dist/index.full.js",
  "jsdelivr": "dist/index.full.js",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.mjs"
    },
    "./es": "./es/index.mjs",
    "./lib": "./lib/index.js",
    "./es/*.mjs": "./es/*.mjs",
    "./es/*": "./es/*.mjs",
    "./lib/*.js": "./lib/*.js",
    "./lib/*": "./lib/*.js",
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "style": "dist/index.css",
  "scripts": {},
  "peerDependencies": {
    "vue": "^3.2.0"
  }
}
